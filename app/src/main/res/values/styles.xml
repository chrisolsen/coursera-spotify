<resources>

    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primary_dark</item>
        <item name="colorAccent">@color/color_accent</item>
    </style>

    <style name="AppTheme.TransparentActionBar">
        <item name="colorPrimary">@color/transparent_black</item>
        <item name="colorPrimaryDark">@color/transparent_black_dark</item>
        <item name="colorAccent">@color/color_accent</item>
    </style>

    <color name="transparent_black_dark">#55000000</color>
    <color name="transparent_black">#55000000</color>
</resources>
